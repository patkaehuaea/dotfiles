
order += "cpu_usage"
order += "volume master"
order += "wireless wlp5s0"
order += "tztime local"

cpu_usage {
  format = "CPU %usage {%cpu0 %cpu1}"
  max_threshold = 75
}

volume master {
  device = "pulse:alsa_output.pci-0000_0b_00.1.hdmi-stereo-extra4"
  format = "♪: %volume"
  format_muted = "♪: muted (%volume)"
}

wireless  wlp5s0 {
  format_up = "(%quality at %essid, %bitrate / %frequency)"
  format_down = "%03d%s"
}

tztime local {
        format = "%Y-%m-%d %H:%M:%S"
}
